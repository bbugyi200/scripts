#!/bin/python
import sys

from twilio.rest import Client

from keys import TWILIO_ACCOUNT_SID, TWILIO_AUTH_TOKEN


body = sys.argv[1]
client = Client(TWILIO_ACCOUNT_SID, TWILIO_AUTH_TOKEN)

message = client.api.account.messages.create(to="+16095007081",
                                             from_="+12316749266",
                                             body=body)
