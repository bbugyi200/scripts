#!/bin/bash

###################################################################################################
#  Shorten TimeWarrior Interval of @1 (last event)                                                #
###################################################################################################

# ---------- Modules ----------
source /home/bryan/Dropbox/scripts/modules/bash/gutils.sh

# ---------- Global Variables ----------
scriptname="$(basename "$0")"

# shellcheck disable=SC2034
usage="usage: ${scriptname} [-d] [-h] [-q]"

# ---------- Main ----------
task start.any: stop &> /dev/null

sleep 0.5
timew shorten @1 "$1"mins
