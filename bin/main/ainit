#!/bin/bash

###########################
#  Initialize AWK Script  #
###########################

source /home/bryan/Dropbox/scripts/templates/gutils.sh

read -r -d '' TEMPLATE << EOM
#!/bin/awk


EOM

cd /home/bryan/Dropbox/scripts/bin/awk || exit 1
printf "$TEMPLATE" > "$1.awk"
chmod +x "$1.awk"
clinks
vim + "$1.awk"
